{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "klarna-checkout": {
      "projectType": "library",
      "root": "projects/klarna-checkout",
      "sourceRoot": "projects/klarna-checkout/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/klarna-checkout/tsconfig.lib.json",
            "project": "projects/klarna-checkout/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/klarna-checkout/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/klarna-checkout/src/test.ts",
            "tsConfig": "projects/klarna-checkout/tsconfig.spec.json",
            "karmaConfig": "projects/klarna-checkout/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/klarna-checkout/tsconfig.lib.json",
              "projects/klarna-checkout/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "klarna-checkout": {
      "projectType": "library",
      "root": "projects/klarna-checkout",
      "sourceRoot": "projects/klarna-checkout/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/klarna-checkout/tsconfig.lib.json",
            "project": "projects/klarna-checkout/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/klarna-checkout/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/klarna-checkout/src/test.ts",
            "tsConfig": "projects/klarna-checkout/tsconfig.spec.json",
            "karmaConfig": "projects/klarna-checkout/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/klarna-checkout/tsconfig.lib.json",
              "projects/klarna-checkout/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "klarna-checkout-root": {
      "projectType": "library",
      "root": "projects/klarna-checkout-root",
      "sourceRoot": "projects/klarna-checkout-root/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/klarna-checkout-root/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/klarna-checkout-root/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/klarna-checkout-root/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/klarna-checkout-root/src/test.ts",
            "tsConfig": "projects/klarna-checkout-root/tsconfig.spec.json",
            "karmaConfig": "projects/klarna-checkout-root/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "klarna-checkout"
}
